//GFG question 
//Given an array A[] of size n. The task is to find the largest element in it.

class Solution
{
public:
    int largest(vector<int> &arr, int n)
    {
        int m= INT_MIN;
        for(int i=0;i<n;i++){
            if(m<arr[i]){
                m=arr[i];
            }
        }
        return m;
    }
};
